{
    "swagger": "2.0",
    "info": {
        "description": "AI Agent OS 存储服务 API 文档",
        "title": "AI Agent OS Storage API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:9092",
    "basePath": "/api/v1",
    "paths": {
        "/api/v1/storage/batch_delete": {
            "post": {
                "description": "批量删除某个函数路径下的所有文件（危险操作）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "删除函数路径下的所有文件",
                "parameters": [
                    {
                        "description": "删除请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.DeleteFilesByRouterReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "删除成功",
                        "schema": {
                            "$ref": "#/definitions/dto.DeleteFilesByRouterResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/batch_upload_complete": {
            "post": {
                "description": "批量通知后端创建上传记录（仅在上传成功时记录）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "批量上传完成通知",
                "parameters": [
                    {
                        "description": "批量上传完成请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.BatchUploadCompleteReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "通知成功",
                        "schema": {
                            "$ref": "#/definitions/dto.BatchUploadCompleteResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/batch_upload_token": {
            "post": {
                "description": "批量获取多个文件的上传凭证，支持多种存储方式（presigned_url/form_upload/sdk_upload）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "批量获取上传凭证",
                "parameters": [
                    {
                        "description": "批量获取上传凭证请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.BatchGetUploadTokenReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "$ref": "#/definitions/dto.BatchGetUploadTokenResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/download/{key}": {
            "get": {
                "description": "直接代理下载文件，返回文件流（无需复杂的预签名 URL 参数）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/octet-stream"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "下载文件",
                "parameters": [
                    {
                        "type": "string",
                        "description": "文件 Key",
                        "name": "key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "文件内容",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/files": {
            "get": {
                "description": "列举某个函数路径下的所有文件",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "列举文件",
                "parameters": [
                    {
                        "type": "string",
                        "description": "函数路径",
                        "name": "router",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "$ref": "#/definitions/dto.ListFilesResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/files/{key}": {
            "delete": {
                "description": "删除存储的文件",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "删除文件",
                "parameters": [
                    {
                        "type": "string",
                        "description": "文件 Key",
                        "name": "key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "删除成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/files/{key}/info": {
            "get": {
                "description": "获取文件的元数据信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "获取文件信息",
                "parameters": [
                    {
                        "type": "string",
                        "description": "文件 Key",
                        "name": "key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "$ref": "#/definitions/dto.GetFileInfoResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/stats": {
            "get": {
                "description": "获取某个函数路径下的文件数量和总大小",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "获取存储统计信息",
                "parameters": [
                    {
                        "type": "string",
                        "description": "函数路径，例如：luobei/test88888/tools/cashier_desk",
                        "name": "router",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "$ref": "#/definitions/dto.GetStorageStatsResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/upload_complete": {
            "post": {
                "description": "前端上传完成后，通知后端创建上传记录（仅在上传成功时记录）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "上传完成通知",
                "parameters": [
                    {
                        "description": "上传完成请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.UploadCompleteReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "通知成功",
                        "schema": {
                            "$ref": "#/definitions/dto.UploadCompleteResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/storage/upload_token": {
            "post": {
                "description": "获取文件上传的预签名 URL，文件将按函数路径分类存储",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "存储管理"
                ],
                "summary": "获取上传凭证",
                "parameters": [
                    {
                        "description": "获取上传凭证请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.GetUploadTokenReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "$ref": "#/definitions/dto.GetUploadTokenResp"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.BatchGetUploadTokenReq": {
            "type": "object",
            "required": [
                "files"
            ],
            "properties": {
                "files": {
                    "description": "文件列表（最多100个）",
                    "type": "array",
                    "maxItems": 100,
                    "minItems": 1,
                    "items": {
                        "$ref": "#/definitions/dto.GetUploadTokenReq"
                    }
                },
                "upload_source": {
                    "description": "✨ 上传来源：browser（浏览器）或 server（服务端），默认为 browser",
                    "allOf": [
                        {
                            "$ref": "#/definitions/dto.UploadSource"
                        }
                    ]
                }
            }
        },
        "dto.BatchGetUploadTokenResp": {
            "type": "object",
            "properties": {
                "tokens": {
                    "description": "上传凭证列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.GetUploadTokenResp"
                    }
                }
            }
        },
        "dto.BatchUploadCompleteItem": {
            "type": "object",
            "required": [
                "key"
            ],
            "properties": {
                "content_type": {
                    "description": "✨ 文件类型（上传成功后需要，用于记录）",
                    "type": "string"
                },
                "error": {
                    "description": "错误信息（如果失败）",
                    "type": "string"
                },
                "file_name": {
                    "description": "✨ 文件名（上传成功后需要，用于记录）",
                    "type": "string"
                },
                "file_size": {
                    "description": "✨ 文件大小（上传成功后需要，用于记录）",
                    "type": "integer"
                },
                "hash": {
                    "description": "✨ 文件hash（可选，用于秒传）",
                    "type": "string"
                },
                "key": {
                    "description": "文件 Key",
                    "type": "string"
                },
                "router": {
                    "description": "✨ 函数路径（上传成功后需要，用于记录）",
                    "type": "string"
                },
                "success": {
                    "description": "是否成功",
                    "type": "boolean"
                }
            }
        },
        "dto.BatchUploadCompleteReq": {
            "type": "object",
            "required": [
                "items"
            ],
            "properties": {
                "items": {
                    "description": "上传结果列表（最多100个）",
                    "type": "array",
                    "maxItems": 100,
                    "minItems": 1,
                    "items": {
                        "$ref": "#/definitions/dto.BatchUploadCompleteItem"
                    }
                }
            }
        },
        "dto.BatchUploadCompleteResp": {
            "type": "object",
            "properties": {
                "results": {
                    "description": "处理结果列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.BatchUploadCompleteResult"
                    }
                }
            }
        },
        "dto.BatchUploadCompleteResult": {
            "type": "object",
            "properties": {
                "download_url": {
                    "description": "✨ 外部访问的下载地址（前端使用）",
                    "type": "string"
                },
                "error": {
                    "description": "错误信息（如果失败）",
                    "type": "string"
                },
                "hash": {
                    "description": "✨ 文件hash（用于文件缓存去重）",
                    "type": "string"
                },
                "key": {
                    "description": "文件 Key",
                    "type": "string"
                },
                "server_download_url": {
                    "description": "✨ 内部访问的下载地址（服务端使用）",
                    "type": "string"
                },
                "status": {
                    "description": "状态：completed/failed",
                    "type": "string"
                }
            }
        },
        "dto.DeleteFilesByRouterReq": {
            "type": "object",
            "required": [
                "router"
            ],
            "properties": {
                "router": {
                    "description": "函数路径",
                    "type": "string"
                }
            }
        },
        "dto.DeleteFilesByRouterResp": {
            "type": "object",
            "properties": {
                "deleted_count": {
                    "type": "integer"
                },
