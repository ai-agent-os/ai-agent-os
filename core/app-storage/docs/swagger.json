        },
        "dto.GetFileInfoResp": {
            "type": "object",
            "properties": {
                "content_type": {
                    "type": "string"
                },
                "etag": {
                    "type": "string"
                },
                "key": {
                    "type": "string"
                },
                "last_modified": {
                    "type": "string"
                },
                "size": {
                    "type": "integer"
                }
            }
        },
        "dto.GetStorageStatsResp": {
            "type": "object",
            "properties": {
                "file_count": {
                    "type": "integer"
                },
                "router": {
                    "type": "string"
                },
                "size_human": {
                    "description": "人类可读的大小，例如：10.5 MB",
